<Window x:Class="Курсовой.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Курсовой"
        mc:Ignorable="d"
        Title="Администратор" Height="250" Width="500">
    <Window.Background>
        <ImageBrush ImageSource="fonadm1.png"/>
    </Window.Background>
    <Grid Margin="0,0,2,-2">
        <Label x:Name="label" Content="Введите пароль чтобы войти в&#xD;&#xA; панель администратора(123)" Margin="77,10,43,0" VerticalAlignment="Top" Height="94" Foreground="#FFFB5F5F" FontFamily="GENISO" FontSize="22" FontWeight="Bold"/>
        <Button x:Name="button" Content="Отменить" HorizontalAlignment="Left" Margin="42,169,0,0" VerticalAlignment="Top" Width="100" Height="28" Background="#7FFF7070" Click="button_Click" Foreground="Black" Style="{DynamicResource ButtonStyle1}"/>
        <Button x:Name="button1" Content="ОК" HorizontalAlignment="Left" Margin="347,169,0,0" VerticalAlignment="Top" Width="100" Height="28" Background="#7FFF7070" Click="button1_Click" Style="{DynamicResource ButtonStyle1}"/>
        <PasswordBox x:Name="passwordBox" Margin="102,104,101,0" VerticalAlignment="Top" Height="25" Background="#70FBA7A7"/>
    </Grid>
</Window>
